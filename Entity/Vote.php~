<?php

namespace GS\VoteBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Vote
 *
 * @ORM\Table(name="vote")
 * @ORM\Entity(repositoryClass="GS\VoteBundle\Entity\VoteRepository")
 */
class Vote
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }


    /**
     * @ORM\ManyToOne(targetEntity="GS\UtilisateurBundle\Entity\Utilisateur", inversedBy="projects")
     * @ORM\JoinColumn(nullable=true)
     */

    private $utilisateur;

    /**
     * @ORM\ManyToOne(targetEntity="GS\VoteBundle\Entity\Project")
     * @ORM\JoinColumn(nullable=false)
     */

    private $project;
}
